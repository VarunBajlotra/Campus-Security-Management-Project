    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
    <style>
        nav div ul li a{
            color: white
        }
    </style>
</head>
<body>

    <nav class="navbar navbar-expand-sm bg-dark fixed-top">
        <a href="" class="navbar-brand" style="color:white">
            <img src='../../images/icons8-air-force-commander-male-48.png' height="25">
            Campus Security Management
        </a>

        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarCollapse">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div id="navbarCollapse" class="collapse navbar-collapse">
        <ul class="navbar-nav ml-auto">
            <li class="nav-item">
                <a href="/personnel/profile" class="nav-link active">
                    Profile
                </a>
            </li>
            <li class="nav-item">
                <span id="person" class="nav-link active" style="color:rgb(252, 249, 88)">
                    Welcome, 
                </span>
            </li>
            <li class="nav-item">
                <a href="/all/logout" class="nav-link active">
                    Log Out
                </a>
            </li>
        </ul>
        </div>
    </nav>

    <div class="container-contact100">
		<div class="wrap-contact100">
			<form class="contact100-form validate-form" action='/all/filecomplaint' method="POST">
				<span class="contact100-form-title">
					Complaint Details
				</span>

				<label class="label-input100" for="location">Enter Place</label>
				<div class="wrap-input100 validate-input" data-validate = "Valid place is required">
					<input id="location" class="input100" type="text" name="location" placeholder="Enter Your Location">
					<span class="focus-input100"></span>
				</div>

				<label class="label-input100" for="message">Complaint Description</label>
				<div class="wrap-input100 validate-input" data-validate = "Message is required">
					<textarea id="description" class="input100" name="description" placeholder="Describe Your Complaint"></textarea>
					<span class="focus-input100"></span>
				</div>

				<div class="container-contact100-form-btn">
					<button class="contact100-form-btn">
						Send Complaint
					</button>
				</div>
			</form>

			<div class="contact100-more flex-col-c-m" style="background-image: url('../filecomplaint/images/bg-01.jpg');">
				<div class="flex-w size1 p-b-47">
					<div class="txt1 p-r-25">
						<span class="lnr lnr-map-marker"></span>
					</div>

					<div class="flex-col size2">
						<span class="txt1 p-b-20">
							Address
						</span>

						<span class="txt2">
							Delhi Technological University<br>Shahbad Daulatpur, Main Bawana Road Delhi-110042
						</span>
					</div>
				</div>

				<div class="dis-flex size1 p-b-47">
					<div class="txt1 p-r-25">
						<span class="lnr lnr-phone-handset"></span>
					</div>

					<div class="flex-col size2">
						<span class="txt1 p-b-20">
							Lets Talk
						</span>

						<span class="txt3">
							+91 8130706616
						</span>
					</div>
				</div>

				<div class="dis-flex size1 p-b-47">
					<div class="txt1 p-r-25">
						<span class="lnr lnr-envelope"></span>
					</div>

					<div class="flex-col size2">
						<span class="txt1 p-b-20">
							General Support
						</span>

						<span class="txt3">
							campussecurity@dtu.ac.in
						</span>
					</div>
				</div>
			</div>
		</div>
	</div>



	<div id="dropDownSelect1"></div>

    <!--===============================================================================================-->
	<script src="../filecomplaint/vendor/jquery/jquery-3.2.1.min.js"></script>
<!--===============================================================================================-->
