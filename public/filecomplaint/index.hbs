<!--===============================================================================================-->
	<script src="../filecomplaint/vendor/bootstrap/js/popper.js"></script>
	<script src="../filecomplaint/vendor/bootstrap/js/bootstrap.min.js"></script>
<!--===============================================================================================-->
	<script src="../filecomplaint/vendor/select2/select2.min.js"></script>
	<script>
		$(".selection-2").select2({
			minimumResultsForSearch: 20,
			dropdownParent: $('#dropDownSelect1')
		});
	</script>
<!--===============================================================================================-->
	<script src="../filecomplaint/vendor/daterangepicker/moment.min.js"></script>
	<script src="../filecomplaint/vendor/daterangepicker/daterangepicker.js"></script>
<!--===============================================================================================-->
